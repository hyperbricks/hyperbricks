==== hyperbricks config {!{image}} ====
image = <IMAGE>
image.src = hyperbricks-test-files/assets/cute_cat.jpg
image.width = 100
# Use image.width and image.height for forcing aspect ratio 
image.title = Some Cute Cat!
image.class = class-a class-b class-c
image.attributes {
  usemap = #catmap 
}
image.alt = cat but cute
image.quality = 90
image.id = #cat
